$headers = New-Object "System.Collections.Generic.Dictionary[[String],[String]]"
$headers.Add("Authorization", "Bearer <token>")

$response = Invoke-RestMethod 'https://ontology.scot.nhs.uk/production1/fhir/ValueSet?url:below=https://dmd.nhs.uk&_elements=url,version' -Method 'GET' -Headers $headers
$response | ConvertTo-Json