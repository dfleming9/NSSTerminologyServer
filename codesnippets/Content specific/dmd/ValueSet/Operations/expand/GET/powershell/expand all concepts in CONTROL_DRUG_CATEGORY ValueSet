$headers = New-Object "System.Collections.Generic.Dictionary[[String],[String]]"
$headers.Add("Authorization", "Bearer <token>")

$response = Invoke-RestMethod 'https://ontology.scot.nhs.uk/production1/fhir/ValueSet/$expand?url=https://dmd.nhs.uk/CONTROL_DRUG_CATEGORY/vs&count=5&offset=5' -Method 'GET' -Headers $headers
$response | ConvertTo-Json