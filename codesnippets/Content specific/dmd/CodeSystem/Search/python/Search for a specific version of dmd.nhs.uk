import http.client

conn = http.client.HTTPSConnection("ontology.scot.nhs.uk")
payload = ''
headers = {
  'Authorization': 'Bearer <token>'
}
conn.request("GET", "/production1/fhir/CodeSystem?url=https://dmd.nhs.uk&version=202111.1.0", payload, headers)
res = conn.getresponse()
data = res.read()
print(data.decode("utf-8"))