$headers = New-Object "System.Collections.Generic.Dictionary[[String],[String]]"
$headers.Add("Authorization", "Bearer <token>")

$response = Invoke-RestMethod 'https://ontology.scot.nhs.uk/production1/fhir/CodeSystem?url:below=https://dmd.nhs.uk&version=202112.0.0&_count=10&_elements=name,url,version,valueSet' -Method 'GET' -Headers $headers
$response | ConvertTo-Json