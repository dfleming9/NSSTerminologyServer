package main

import (
  "fmt"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://ontology.scot.nhs.uk/production1/fhir/CodeSystem?url:below=https://dmd.nhs.uk&version=202112.0.0&_count=10&_elements=name,url,version,valueSet"
  method := "GET"

  client := &http.Client {
  }
  req, err := http.NewRequest(method, url, nil)

  if err != nil {
    fmt.Println(err)
    return
  }
  req.Header.Add("Authorization", "Bearer <token>")

  res, err := client.Do(req)
  if err != nil {
    fmt.Println(err)
    return
  }
  defer res.Body.Close()

  body, err := ioutil.ReadAll(res.Body)
  if err != nil {
    fmt.Println(err)
    return
  }
  fmt.Println(string(body))
}