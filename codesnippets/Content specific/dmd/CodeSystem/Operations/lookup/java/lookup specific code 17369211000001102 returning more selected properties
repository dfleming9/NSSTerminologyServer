OkHttpClient client = new OkHttpClient().newBuilder()
  .build();
Request request = new Request.Builder()
  .url("https://ontology.scot.nhs.uk/production1/fhir/CodeSystem/$lookup?system=https://dmd.nhs.uk&code=17369211000001102&property=FORMCD&property=ROUTECD&property=version")
  .method("GET", null)
  .addHeader("Authorization", "Bearer <token>")
  .build();
Response response = client.newCall(request).execute();