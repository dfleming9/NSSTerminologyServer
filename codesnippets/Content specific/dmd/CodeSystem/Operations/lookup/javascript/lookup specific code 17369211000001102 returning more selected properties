var myHeaders = new Headers();
myHeaders.append("Authorization", "Bearer <token>");

var requestOptions = {
  method: 'GET',
  headers: myHeaders,
  redirect: 'follow'
};

fetch("https://ontology.scot.nhs.uk/production1/fhir/CodeSystem/$lookup?system=https://dmd.nhs.uk&code=17369211000001102&property=FORMCD&property=ROUTECD&property=version", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));