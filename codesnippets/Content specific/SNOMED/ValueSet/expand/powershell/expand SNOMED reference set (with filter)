$headers = New-Object "System.Collections.Generic.Dictionary[[String],[String]]"
$headers.Add("Authorization", "Bearer <token>")

$response = Invoke-RestMethod 'https://ontology.scot.nhs.uk/production1/fhir/ValueSet/$expand?url=http://snomed.info/sct?fhir_vs=refset/723264001&count=10&filter=myot' -Method 'GET' -Headers $headers
$response | ConvertTo-Json