var myHeaders = new Headers();
myHeaders.append("Authorization", "Bearer <token>");

var requestOptions = {
  method: 'GET',
  headers: myHeaders,
  redirect: 'follow'
};

fetch("https://ontology.scot.nhs.uk/production1/fhir/ValueSet/$validate-code?system=http://snomed.info/sct&code=46836008&url=http://snomed.info/sct?fhir_vs=refset/723264001", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));