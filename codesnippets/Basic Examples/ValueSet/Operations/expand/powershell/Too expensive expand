$headers = New-Object "System.Collections.Generic.Dictionary[[String],[String]]"
$headers.Add("Authorization", "Bearer <token>")

$response = Invoke-RestMethod 'https://ontology.scot.nhs.uk/production1/fhir/ValueSet/$expand?url=http://snomed.info/sct/83821000000107?fhir_vs' -Method 'GET' -Headers $headers
$response | ConvertTo-Json