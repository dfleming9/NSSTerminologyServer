var client = new RestClient("https://ontology.scot.nhs.uk/production1/fhir/ValueSet/$expand?url=http://snomed.info/sct/83821000000107/version/20211124?fhir_vs&filter=ESRD");
client.Timeout = -1;
var request = new RestRequest(Method.GET);
request.AddHeader("Authorization", "Bearer <token>");
IRestResponse response = client.Execute(request);
Console.WriteLine(response.Content);