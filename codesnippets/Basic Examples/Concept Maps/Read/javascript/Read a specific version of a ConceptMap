var myHeaders = new Headers();
myHeaders.append("Authorization", "Bearer <token>");

var requestOptions = {
  method: 'GET',
  headers: myHeaders,
  redirect: 'follow'
};

fetch("https://ontology.scot.nhs.uk/production1/fhir/ConceptMap/NICIP-SNOMED-MAP-20171001/_history/1?_format=application/fhir+xml", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));