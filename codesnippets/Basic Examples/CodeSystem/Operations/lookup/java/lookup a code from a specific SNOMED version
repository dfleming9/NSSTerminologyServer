OkHttpClient client = new OkHttpClient().newBuilder()
  .build();
Request request = new Request.Builder()
  .url("https://ontology.scot.nhs.uk/production1/fhir/CodeSystem/$lookup?system=http://snomed.info/sct&code=56265001&_format=json&version=http://snomed.info/sct/83821000000107/version/20211222")
  .method("GET", null)
  .addHeader("Authorization", "Bearer <token>")
  .build();
Response response = client.newCall(request).execute();